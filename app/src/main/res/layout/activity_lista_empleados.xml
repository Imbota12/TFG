<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".vistas.ListaEmpleados">

    <TextView
        android:id="@+id/textView43"
        android:layout_width="1161dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="616dp"
        android:text="LISTADO DE EMPLEADOS"
        android:background="@color/black"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/excelListaEmpleados"
        android:layout_width="208dp"
        android:layout_height="88dp"
        android:background="@android:color/transparent"
        android:contentDescription="correo"
        android:scaleType="fitCenter"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/generarexcel" />
    <ImageButton
        android:id="@+id/volvermenuAdminn"
        android:layout_width="208dp"
        android:layout_height="88dp"
        android:layout_marginStart="924dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="correo"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/volvermenu" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_listaEmpleados"
        android:layout_width="1042dp"
        android:layout_height="500dp"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="116dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>