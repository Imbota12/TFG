<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    tools:context=".vistas.ListaTiendas">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/listaTienda"
        android:layout_width="800dp"
        android:layout_height="500dp"
        android:layout_marginStart="180dp"
        android:layout_marginBottom="116dp"
        android:background="@color/zxing_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="1161dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="616dp"
        android:text="LISTADO DE TIENDAS"
        android:background="@color/black"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/excelListaTiendas"
        android:layout_width="208dp"
        android:layout_height="88dp"
        android:background="@android:color/transparent"
        android:contentDescription="correo"
        android:scaleType="fitCenter"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/generarexcel" />

    <ImageButton
        android:id="@+id/volvermenuAdmin"
        android:layout_width="208dp"
        android:layout_height="88dp"
        android:layout_marginStart="924dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/transparent"
        android:contentDescription="correo"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/volvermenu" />
</androidx.constraintlayout.widget.ConstraintLayout>